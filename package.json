{
  "name": "chatluthier",
  "version": "1.0.0",
  "description": "Le Chat Luthier is your immersive sound companion for enhancing role-playing games, storytelling sessions, or any experience that benefits from atmospheric audio. Use `npm run local` or `npm run npm:local` to start the local development server.",
  "main": "main.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --watch --config webpack.config.js",
    "start": "webpack --config webpack.config.js && node srv/server.js",
    "dev": "node srv/server.js",
    "local": "npm run dev",
    "npm:local": "npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "babel-loader": "^8.4.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.11.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "style-loader": "^4.0.0",
    "webpack": "^5.96.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "google-auth-library": "^9.15.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "ws": "^8.17.0"
  },
  "keywords": [
    "sound",
    "role-playing",
    "audio",
    "soundboard",
    "ambiance",
    "storytelling",
    "gaming"
  ],
  "author": "Benjamin Thibault",
  "license": "ISC"
}
